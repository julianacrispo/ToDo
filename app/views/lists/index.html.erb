<%= link_to "Create New List", new_list_path %>
<h1>Lists Index</h1>

<div class="row">
  <div class="col-md-8">
    <% @lists.each do |list| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to list.name, list %>
          </h4>
          <small>
            <%= list.description %>
      
          </small>
        </div>
      </div>
    <% end %>
  </div>
</div>